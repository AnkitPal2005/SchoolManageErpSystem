@model List<StudentListViewModel>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>RollNumber</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var s in Model)
        {
            <tr>
                <td>@s.FullName</td>
                <td>@s.Email</td>
                <td>@s.PhoneNumber</td>
                <td>@s.RollNumber</td>
                <td>
                    <button class="btn btn-sm btn-warning" onclick="editStudent('@s.UserId')">Edit</button>
                    <button class="btn btn-sm btn-danger" onclick="deleteStudent('@s.UserId')">Delete</button>
                </td>
            </tr>
        }
    </tbody>
</table>